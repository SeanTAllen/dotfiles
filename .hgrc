[ui]
verbose  = true
merge    = emacs

[merge-tools]
emacs.args = -q --eval "(ediff-merge-with-ancestor \"$local\" \"$other\" \"$base\" nil \"$output\")"

[alias]
nudge = push --rev .

[extensions]
color      =
convert    =
crecord    = ~/.hgext/crecord/crecord
graphlog   =
hgflow     = ~/.hgext/hgflow/hgflow.py
hgshelve   = ~/.hgext/hgshelve/hgshelve.py
histedit   = ~/.hgext/histedit/hg_histedit.py
mq         =
rebase     =
rebaseif   = ~/.hgext/ml-hgext/rebaseif.py
transplant =
